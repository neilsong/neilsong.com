{
  "tabWidth": 2,
  "semi": true,
  "printWidth": 90,
  "singleQuote": false,
  "endOfLine": "lf",
  "trailingComma": "es5",
  "quoteProps": "preserve",
  "overrides": [
    {
      "files": ["**/*.jsonc", "**/tsconfig*.json"],
      "options": {
        "trailingComma": "none"
      }
    }
  ],
  "plugins": ["prettier-plugin-organize-imports", "@prettier/plugin-oxc"]
}
